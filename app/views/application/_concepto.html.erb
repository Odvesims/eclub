<% if @concepto_existe == nil %>
	<td><%= text_field_tag(:codigo, "", :id => "concepto_cod", :size => 7, :onchange=>"validacodsim(this,'" + action_name+ "','" + controller_name + "')") %>
	<%= hidden_field_tag(:concepto_id, "", :id => "concepto_id", :size => 7) %>
	<%= image_tag "SearchIcon2.png", :onclick=>"selectModel('concepto','busconcepto','"+ controller_name + "')", :height =>'20', :width => '20', :title=>"Buscar concepto", :class=> "pin_image" %></td>
	<td colspan="5"><span id="divsimnun" > </span></td>
<% else %>
	<td><%= text_field_tag(:codigo, @concepto_existe.codigo, :id => "concepto_cod", :size => 10, :onchange=>"validacodsim(this,'" + action_name+ "','" + controller_name + "')") %>
	<%= hidden_field_tag(:concepto_id, @concepto_existe.id, :id => "concepto_id", :size => 10) %>
	<%= image_tag "SearchIcon2.png", :onclick=>"selectModel('concepto','busconcepto','"+ controller_name + "')", :height =>'20', :width => '20', :title=>"Buscar concepto", :class=> "pin_image" %></td>
<%end%>