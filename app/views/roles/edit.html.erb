
<%= render 'form' %>

<div>

<button id="button" onclick="window.location = '<%= roles_path %>';" class="ui-state-default ui-corner-all" title="Volver a Buscar"><span class="ui-icon ui-icon-circle-zoomout"></button> 
<button id="button"  onclick="window.location = '<%= edit_role_path(@rol,:donde => 'prev') %>';" class="ui-state-default ui-corner-all" title="Anterior"><span class="ui-icon ui-icon-arrowthick-1-w"></button> 
<button id="button"  onclick="window.location = '<%= edit_role_path(@rol,:donde => 'next')%>';" class="ui-state-default ui-corner-all" title="Siguiente"><span class="ui-icon ui-icon-arrowthick-1-e"></button> 
<button id="button"  onclick="window.location = '<%= new_role_path%>';" class="ui-state-default ui-corner-all" title="Nuevo"><span class="ui-icon ui-icon-plusthick"></button> 
</div>

 <script>	 $( "button" ).button(); </script>


