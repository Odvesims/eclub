<ul class="nav nav-tabs">
	<li class="active"><a data-toggle="tab" href="#home"><h3>Camporees</h3></a></li>
</ul>
<div class="tab-content">
	<button type="button" class="btn btn-primary" onclick="window.location = '<%= new_camporee_path %>'">Nuevo</button>
	<table class="table table-striped">
		<thead>
		  <tr>
			<th>ID</th>
			<th>Tipo</th>
			<th>Nombre</th>
			<th>Fecha</th>
			<th>Campo</th>
		  </tr>
		</thead>
		<tbody>
			<% @camporees.each do |c| %>
				<tr>
					<td><%= c.id %></td>
					<td><%= c.camporee_tipo %></td>
					<td><%= link_to c.nombre, edit_camporee_path(c.id) %></td>
					<td><%= c.fecha %></td>
					<td><%= c.campo_nombre %></td>
				</tr>
			<% end %>
		</tbody>
	</table>
</div>